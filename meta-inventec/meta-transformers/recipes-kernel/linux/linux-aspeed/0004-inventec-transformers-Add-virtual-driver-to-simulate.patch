From b9b1d4843a5ce655b135ac3099a2a994d92688d1 Mon Sep 17 00:00:00 2001
From: Neil Chen <Chen.NeilZX@inventec.com>
Date: Fri, 5 Feb 2021 06:21:46 +0000
Subject: [PATCH] [inventec][transformers]Add virtual driver to simulate driver
 behavier

---
 drivers/Kconfig         | 2 ++
 drivers/Makefile        | 1 +
 drivers/virtual/Kconfig | 9 +++++++++
 3 files changed, 12 insertions(+)
 create mode 100644 drivers/virtual/Kconfig

diff --git a/drivers/Kconfig b/drivers/Kconfig
index 7ecac13..c447300 100644
--- a/drivers/Kconfig
+++ b/drivers/Kconfig
@@ -240,4 +240,6 @@ source "drivers/peci/Kconfig"
 
 source "drivers/jtag/Kconfig"
 
+source "drivers/virtual/Kconfig"
+
 endmenu
diff --git a/drivers/Makefile b/drivers/Makefile
index b8ba36d..705b31c 100644
--- a/drivers/Makefile
+++ b/drivers/Makefile
@@ -190,3 +190,4 @@ obj-$(CONFIG_COUNTER)		+= counter/
 obj-$(CONFIG_MOST)		+= most/
 obj-$(CONFIG_PECI)		+= peci/
 obj-$(CONFIG_JTAG)              += jtag/
+obj-$(CONFIG_VIRTUAL)		+= virtual/
diff --git a/drivers/virtual/Kconfig b/drivers/virtual/Kconfig
new file mode 100644
index 0000000..97f314c
--- /dev/null
+++ b/drivers/virtual/Kconfig
@@ -0,0 +1,9 @@
+menuconfig VIRTUAL
+	tristate "VIRTUAL support"
+	help
+	  This provides virtual driver to create hwmon or else, to simulate driver behavier.
+
+	  If you want this support, you should say Y here.
+
+	  To compile this driver as a module, choose M here: the module will
+	  be called virtual.
\ No newline at end of file
-- 
2.7.4

