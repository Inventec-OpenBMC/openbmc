diff --git a/CMakeLists.txt b/CMakeLists.txt
index 83d2dcd..e5f93b1 100644
--- a/CMakeLists.txt
+++ b/CMakeLists.txt
@@ -46,8 +46,9 @@ add_definitions (-DBOOST_NO_RTTI)
