[Unit]
Description=UnPlug PSU Inventory for PSU ID %I
Wants=mapper-wait@-xyz-openbmc_project-inventory.service
After=mapper-wait@-xyz-openbmc_project-inventory.service

[Service]
Type=oneshot
RemainAfterExit=no
ExecStart=/usr/sbin/inv-psu-unplug.sh %i
SyslogIdentifier=inv-psu-unplug

[Install]
WantedBy=multi-user.target